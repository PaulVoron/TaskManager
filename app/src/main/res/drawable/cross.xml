<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">

        <shape
            android:shape="rectangle">
            <solid android:color="#000000" />
            <size
                android:width="2dp"
                android:height="14dp" />
        </shape>
    </item>
    <item
        android:bottom="6dp"
        android:top="6dp">
        <shape
            android:shape="rectangle">
            <solid android:color="#000000" />
            <size
                android:width="14dp"
                android:height="2dp" />
        </shape>

    </item>

</layer-list>
